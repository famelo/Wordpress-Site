{
  "variables": [
    {
      "name": "template_name",
      "question": "Please specify a name for the template",
      "default": "template"
    }
  ],
  "submodules": [
    {
      "module": "famelo/Wordpress",
      "path": "wp-content/themes/{{template_name}}",
      "variables": {
        "name": "{{template_name}}"
      }
    },
    {
      "module": "famelo/Wordpress-Plugin-famelo-domain-change",
      "path": "wp-content/plugins/famelo-domain-change"
    }
  ]
}
